{
  "name": "webtheory/stringmaker",
  "description": "Dynamic generation of string values from any context",
  "homepage": "https://github.com/spider-mane/stringmaker",
  "license": "MIT",
  "keywords": [
    "WebTheory",
    "stringmaker"
  ],
  "authors": [
    {
      "name": "Chris Williams",
      "email": "spider.mane.web@gmail.com",
      "homepage": "https://github.com/spider-mane",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "filp/whoops": "^2.5",
    "friendsofphp/php-cs-fixer": "^2.15",
    "phpbench/phpbench": "@dev",
    "phpunit/phpunit": "^8.2",
    "psy/psysh": "^0.9.9",
    "spatie/phpunit-watcher": "^1.12",
    "squizlabs/php_codesniffer": "^3.0",
    "symfony/var-dumper": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "WebTheory\\Stringmaker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "watch": "phpunit-watcher watch",
    "coverage": "vendor/bin/phpunit --coverage-html coverage",
    "check": "vendor/bin/phpcs src tests",
    "fix": "vendor/bin/phpcbf src tests",
    "bench": "vendor/bin/phpbench"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "process-timeout": 0
  }
}
